{
    "name": "@lapis-lang/lapis-js",
    "version": "0.35.2",
    "description": "An embedded DSL for experimentation of Lapis Semantics",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs"
        }
    },
    "main": "./dist/index.mjs",
    "keywords": [
        "algebraic-data-types",
        "adt",
        "codata",
        "functional-programming",
        "pattern-matching",
        "fold",
        "unfold",
        "catamorphism",
        "anamorphism",
        "lapis"
    ],
    "scripts": {
        "prebuild": "npm run lint:fix",
        "build": "webpack",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "pretest": "npm run build",
        "test": "globstar -- node --experimental-test-coverage --test ./src/tests/*.test.mjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lapis-lang/lapis-js.git"
    },
    "author": "Michael L Haufe <tno@thenewobjective.com> (https://thenewobjective.com)",
    "license": "AGPL-3.0-only",
    "bugs": {
        "url": "https://github.com/lapis-lang/lapis-js/issues"
    },
    "homepage": "https://github.com/lapis-lang/lapis-js#readme",
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@eslint/json": "^0.14.0",
        "@eslint/markdown": "^7.5.1",
        "@types/node": "^24.10.1",
        "eslint": "^9.39.1",
        "eslint-webpack-plugin": "^5.0.2",
        "globals": "^16.5.0",
        "globstar": "^1.0.0",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    },
    "files": [
        "dist"
    ]
}