{
    "name": "@lapis-lang/lapis-js",
    "version": "0.0.1",
    "description": "An embedded DSL for experimentation of Lapis Semantics",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "prebuild": "npm run lint:fix",
        "build": "webpack",
        "lint": "eslint src/ --ext .mts",
        "lint:fix": "eslint src/ --ext .mts --fix",
        "pretest": "npm run build",
        "test": "globstar -- node --import tsx --test ./src/tests/*.test.mts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lapis-lang/lapis-js.git"
    },
    "author": "Michael L Haufe <tno@thenewobjective.com> (https://thenewobjective.com)",
    "license": "AGPL-3.0-only",
    "bugs": {
        "url": "https://github.com/lapis-lang/lapis-js/issues"
    },
    "homepage": "https://github.com/lapis-lang/lapis-js#readme",
    "dependencies": {
        "@eslint/js": "^9.39.1",
        "@eslint/json": "^0.14.0",
        "@eslint/markdown": "^7.5.1",
        "@types/node": "^24.10.1",
        "eslint": "^9.39.1",
        "eslint-webpack-plugin": "^5.0.2",
        "globals": "^16.5.0",
        "globstar": "^1.0.0",
        "ts-loader": "^9.5.4",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.4",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    },
    "files": [
        "dist"
    ]
}